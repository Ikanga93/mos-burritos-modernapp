{
  "version": 2,
  "env": {
    "ENVIRONMENT": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/index"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    }
  ]
}
